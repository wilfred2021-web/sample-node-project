{
  "name": "sample-node-project",
  "version": "1.0.0",
  "description": "Sample Node.js Project",
  "main": "app.js",
  "scripts": {
    "tests": "mocha",
	"test:awesome": "mocha --reporter mochawesome || true ",
	"open:report": "open mochawesome-report/mochawesome.html",
	"sonar":"sonar-scanner",
	"coverage": "./node_modules/.bin/nyc -r cobertura -r text-summary -r html npm test",
	"coverage-lcov": "./node_modules/.bin/nyc -r cobertura -r text-summary -r lcov npm test",
	"start": "nodemon src/app.js",
	"build": "webpack -p",
	"lint": "./node_modules/.bin/eslint ."
  },
  "keywords": [],
  "author": "Adem Cemil YALCIN",
  "license": "ISC"
  "devDependencies": {
    "dotenv": "5.0.1",
	"eslint": "7.11.0",
	"eslint-config-airbnb-base": "14.2.0",
	"eslint-plugin-import": "2.22.1",
	"mocha": "8.2.0",
	"nock": "13.0.4",
	"nyc": "15.1.0",
	"sonar-scanner":"3.1.0",
	"supertest": "3.0.0"
	}
}
